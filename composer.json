{
    "name": "gpupo/graphql-rest-wrap",
    "description": "Wrap a REST API with GraphQL",
    "homepage": "https://opensource.gpupo.com/graphql-rest-wrap/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["rest", "graphql"],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "gpupo/common": "^5.0",
        "gpupo/common-sdk": "*"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\GraphqlRestWrap\\": "src",
            "Gpupo\\GraphqlRestWrap\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "gpupo/common-dev": "^5.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
